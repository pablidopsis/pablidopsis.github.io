@article{dixon_vernalization1_2019-1,
	title = {{VERNALIZATION1} controls developmental responses of winter wheat under high ambient temperatures},
	volume = {146},
	issn = {1477-9129, 0950-1991},
	url = {https://journals.biologists.com/dev/article/146/3/dev172684/48961/VERNALIZATION1-controls-developmental-responses-of},
	doi = {10.1242/dev.172684},
	abstract = {Low temperatures are required to regulate the transition from vegetative to reproductive growth via a pathway called vernalization. In wheat, vernalization predominantly involves the cold upregulation of the floral activator VERNALIZATION1 (VRN1). Here, we have used an extreme vernalization response, identified through studying ambient temperature responses, to reveal the complexity of temperature inputs into VRN-A1, with allelic inter-copy variation at a gene expansion of VRN-A1 modulating these effects. We find that the repressors of the reproductive transition, VERNALIZATION2 (VRN2) and ODDSOC2, are re-activated when plants experience high temperatures during and after vernalization. In addition, this re-activation is regulated by photoperiod for VRN2 but was independent of photoperiod for ODDSOC2. We also find this warm temperature interruption affects flowering time and floret number and is stage specific. This research highlights the important balance between floral activators and repressors in coordinating the response of a plant to temperature, and that the absence of warmth is essential for the completion of vernalization. This knowledge can be used to develop agricultural germplasm with more predictable vernalization responses that will be more resilient to variable growth temperatures.},
	language = {en},
	number = {3},
	urldate = {2023-09-11},
	journal = {Development},
	author = {Dixon, Laura E. and Karsai, Ildiko and Kiss, Tibor and Adamski, Nikolai M. and Liu, Zhenshan and Ding, Yiliang and Allard, Vincent and Boden, Scott A. and Griffiths, Simon},
	month = feb,
	year = {2019},
	pages = {dev172684},
	file = {Dixon et al. - 2019 - VERNALIZATION1 controls developmental respo.pdf:C\:\\Users\\pagos\\Zotero\\storage\\8KEHX4FI\\Dixon et al. - 2019 - VERNALIZATION1 controls developmental respo.pdf:application/pdf},
}

@article{prieto_physiological_2018,
	title = {Physiological determinants of fertile floret survival in wheat as affected by earliness per se genes under field conditions},
	volume = {99},
	issn = {1161-0301},
	url = {https://www.sciencedirect.com/science/article/pii/S116103011830306X},
	doi = {10.1016/j.eja.2018.07.008},
	abstract = {Variations in wheat yield are largely explained by changes in grain number per m2 which is linked to the number of fertile florets at anthesis. This, in turn is the outcome of developmental processes which control floret initiation and mortality. Earliness ‘per se’ (Eps) genes are involved in fine-tuning time to anthesis in wheat (and other cereals) but their effect on development prior to anthesis is less well studied. We aimed to determine effects of Eps genes on spike fertility, quantifying aspects of floret developmental which influence this trait. Field experiments were carried out to record floret primordia generation/degeneration dynamics in near isogenic lines (NILs) with contrasting Eps alleles (late flowering vs early flowering alleles; Eps-late and Eps-early, respectively) derived from the Avalon x Cadenza (AxC) cross with the Eps gene on either chromosome 1D or 3 A and from the Spark x Rialto (SxR) cross with the Eps gene on chromosome 1D. Eps NILs varied in spike fertility: Eps-late alleles increased fertility. Although the effect was in general slight, the magnitude was affected by the particular alleles and the cross used to produce the NILs. Differences in the number of fertile florets were explained by differences in the dynamics of floret development. NILs with Eps-late alleles improved the development of a small number of labile florets allowing them to complete their development to become fertile florets instead of dying, as in lines carrying early alleles. Thus, these alleles improved floret fertility mainly through reducing the rate of floret mortality with no influence on the dynamics of floret primordia initiation or in maximum number of floret primordia. Therefore, Eps genes could be exploited in wheat breeding not only to fine-tune time to anthesis but also to improve spike fertility.},
	urldate = {2023-09-11},
	journal = {European Journal of Agronomy},
	author = {Prieto, Paula and Ochagavía, Helga and Savin, Roxana and Griffiths, Simon and Slafer, Gustavo A.},
	month = sep,
	year = {2018},
	keywords = {Earliness, Fertile floret, Floret development, Late reproductive phase, Spike fertility},
	pages = {206--213},
	file = {ScienceDirect Snapshot:C\:\\Users\\pagos\\Zotero\\storage\\BYVZE4KF\\S116103011830306X.html:text/html},
}

@article{prieto_physiological_2018-1,
	title = {Physiological determinants of fertile floret survival in wheat as affected by earliness per se genes under field conditions},
	volume = {99},
	issn = {11610301},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S116103011830306X},
	doi = {10.1016/j.eja.2018.07.008},
	abstract = {Variations in wheat yield are largely explained by changes in grain number per m2 which is linked to the number of fertile ﬂorets at anthesis. This, in turn is the outcome of developmental processes which control ﬂoret initiation and mortality. Earliness ‘per se’ (Eps) genes are involved in ﬁne-tuning time to anthesis in wheat (and other cereals) but their eﬀect on development prior to anthesis is less well studied. We aimed to determine eﬀects of Eps genes on spike fertility, quantifying aspects of ﬂoret developmental which inﬂuence this trait. Field experiments were carried out to record ﬂoret primordia generation/degeneration dynamics in near isogenic lines (NILs) with contrasting Eps alleles (late ﬂowering vs early ﬂowering alleles; Eps-late and Eps-early, respectively) derived from the Avalon x Cadenza (AxC) cross with the Eps gene on either chromosome 1D or 3 A and from the Spark x Rialto (SxR) cross with the Eps gene on chromosome 1D. Eps NILs varied in spike fertility: Eps-late alleles increased fertility. Although the eﬀect was in general slight, the magnitude was aﬀected by the particular alleles and the cross used to produce the NILs. Diﬀerences in the number of fertile ﬂorets were explained by diﬀerences in the dynamics of ﬂoret development. NILs with Eps-late alleles improved the development of a small number of labile ﬂorets allowing them to complete their development to become fertile ﬂorets instead of dying, as in lines carrying early alleles. Thus, these alleles improved ﬂoret fertility mainly through reducing the rate of ﬂoret mortality with no inﬂuence on the dynamics of ﬂoret primordia initiation or in maximum number of ﬂoret primordia. Therefore, Eps genes could be exploited in wheat breeding not only to ﬁne-tune time to anthesis but also to improve spike fertility.},
	language = {en},
	urldate = {2023-09-11},
	journal = {European Journal of Agronomy},
	author = {Prieto, Paula and Ochagavía, Helga and Savin, Roxana and Griffiths, Simon and Slafer, Gustavo A.},
	month = sep,
	year = {2018},
	keywords = {read},
	pages = {206--213},
	annote = {Findings


(Some) Eps genes affect not only the timing of anthesis but also the number of fertile florets.


Plants carrying late flowering Eps alleles tend to have slightly more fertile florets.


This is particularly notable in the central spikelets. 


This is, according to the authors, due to a REDUCED FLORET MORTALITY, which is caused by an IMPROVED DEVELOPMENT of labile florets when the Eps allele was in the late form.


So, essentially, for lines with different Eps alleles, differences in the number of ‘surviving’ (fertile) florets are due to the degree of floret development.


(My interpretation) For a floret not to die, it needs to reach a certain developmental stage.


},
	file = {Prieto et al. - 2018 - Physiological determinants of fertile floret survi.pdf:C\:\\Users\\pagos\\Zotero\\storage\\J9JFTJJ3\\Prieto et al. - 2018 - Physiological determinants of fertile floret survi.pdf:application/pdf},
}

