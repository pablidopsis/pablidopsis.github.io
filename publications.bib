	author = {Xin, Zhanguo and Mandaokar, Ajin and Chen, Junping and Last, Robert L. and Browse, John},
	year = {2007},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/j.1365-313X.2006.02994.x},
	keywords = {cold acclimation, freezing tolerance, esk1 mutant, microarray},
	pages = {786--799},
	file = {Full Text PDF:C\:\\Users\\pagos\\Zotero\\storage\\VB7WIU95\\Xin et al. - 2007 - Arabidopsis ESK1 encodes a novel regulator of free.pdf:application/pdf;Snapshot:C\:\\Users\\pagos\\Zotero\\storage\\CDIDL8BJ\\j.1365-313X.2006.02994.html:text/html},
