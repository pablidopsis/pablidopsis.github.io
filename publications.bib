	year = {2008},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/j.1365-3040.2007.01763.x},
	keywords = {read, Arabidopsis, chilling, expression arrays, metabolite profile, mRNA data, protein data},
