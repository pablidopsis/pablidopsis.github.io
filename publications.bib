	keywords = {ERF5, jasmonic acid (JA), MED16, Mediator, Pseudomonas syringae, salicylic acid (SA), SFR6, UV-C},
	pages = {217--230},
	file = {Full Text PDF:C\:\\Users\\pagos\\Zotero\\storage\\9U7K3HJ2\\Wathugala et al. - 2012 - The Mediator subunit SFR6MED16 controls defence g.pdf:application/pdf;Snapshot:C\:\\Users\\pagos\\Zotero\\storage\\PCEX4FJA\\j.1469-8137.2012.04138.html:text/html},
