@article{dias_de_oliveira_elevated_2015,
	title = {Elevated {CO2} {Reduced} {Floret} {Death} in {Wheat} {Under} {Warmer} {Average} {Temperatures} and {Terminal} {Drought}},
	volume = {6},
	issn = {1664-462X},
	url = {https://www.frontiersin.org/articles/10.3389/fpls.2015.01010},
	abstract = {Elevated CO2 often increases grain yield in wheat by enhancing grain number per ear, which can result from an increase in the potential number of florets or a reduction in the death of developed florets. The hypotheses that elevated CO2 reduces floret death rather than increases floret development, and that grain size in a genotype with more grains per unit area is limited by the rate of grain filling, were tested in a pair of sister lines contrasting in tillering capacity (restricted- vs. free-tillering). The hypotheses were tested under elevated CO2, combined with +3°C above ambient temperature and terminal drought, using specialized field tunnel houses. Elevated CO2 increased net leaf photosynthetic rates and likely the availability of carbon assimilates, which significantly reduced the rates of floret death and increased the potential number of grains at anthesis in both sister lines by an average of 42\%. The restricted-tillering line had faster grain-filling rates than the free-tillering line because the free-tillering line had more grains to fill. Furthermore, grain-filling rates were faster under elevated CO2 and +3°C above ambient. Terminal drought reduced grain yield in both lines by 19\%. Elevated CO2 alone increased the potential number of grains, but a trade-off in yield components limited grain yield in the free-tillering line. This emphasizes the need for breeding cultivars with a greater potential number of florets, since this was not affected by the predicted future climate variables.},
	urldate = {2023-09-12},
	journal = {Frontiers in Plant Science},
	author = {Dias de Oliveira, Eduardo and Palta, Jairo A. and Bramley, Helen and Stefanova, Katia and Siddique, Kadambot H. M.},
	year = {2015},
	keywords = {read, temperature},
	annote = {Background


The amount of C assimilates needed to produce florets is almost insignificant compared with the amount needed to sustain their growth and development. This implies that the availability of C assimilates should benefit floret survival rather than floret production.


Findings


Effect of slight increase in temperature (+3ºC).


Beginning of floret initiation could be a little bit delayed in higher temperature (from the graph).


Rate of floret production unaffected by temperature, it seems.


Maximum number of florets slightly greater in higher temperature. More notable in restricted-tillering lines.


Duration of floret initiation in ºCd seems more or less the same.


Degradation of florets is at the same rate more or less. 


Surviving florets more or less the same, despite differences in the maximum number of florets produced.


Higher temperature hastened the number of floret deaths…


},
	file = {Full Text PDF:C\:\\Users\\pagos\\Zotero\\storage\\T5ZN9VVF\\Dias de Oliveira et al. - 2015 - Elevated CO2 Reduced Floret Death in Wheat Under W.pdf:application/pdf},
}

@article{dreccer_more_2014,
	title = {More fertile florets and grains per spike can be achieved at higher temperature in wheat lines with high spike biomass and sugar content at booting},
