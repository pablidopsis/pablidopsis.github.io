	pages = {259--271},
	file = {Full Text PDF:C\:\\Users\\pagos\\Zotero\\storage\\KAKEPAE2\\Alonso-Blanco et al. - 1998 - Development of an AFLP based linkage map of Ler, C.pdf:application/pdf;Snapshot:C\:\\Users\\pagos\\Zotero\\storage\\PQ3CTMYQ\\j.1365-313X.1998.00115.html:text/html},
