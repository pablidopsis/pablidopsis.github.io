	author = {Swarup, Kamal and Alonso-Blanco, Carlos and Lynn, James R. and Michaels, Scott D. and Amasino, Richard M. and Koornneef, Maarten and Millar, Andrew J.},
	year = {1999},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1046/j.1365-313X.1999.00577.x},
	keywords = {read},
