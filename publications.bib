	pages = {e0171},
	file = {PubMed Central Full Text PDF:C\:\\Users\\pagos\\Zotero\\storage\\SI6FGHHN\\Kim y Sung - 2014 - Genetic and Epigenetic Mechanisms Underlying Verna.pdf:application/pdf},
}

@article{schoof_stem_2000,
	title = {The stem cell population of {Arabidopsis} shoot meristems in maintained by a regulatory loop between the {CLAVATA} and {WUSCHEL} genes},
