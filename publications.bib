	journal = {Plant, Cell \& Environment},
	author = {Bilas, Roza D. and Bretman, Amanda and Bennett, Tom},
	year = {2021},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/pce.13965},
	keywords = {light signalling, plant communication, plant ecology, plant evolution, volatile signalling},
	pages = {997--1013},
	file = {Snapshot:C\:\\Users\\pagos\\Zotero\\storage\\G8PR3RUC\\pce.html:text/html;Versión aceptada:C\:\\Users\\pagos\\Zotero\\storage\\QVPDNPYS\\Bilas et al. - 2021 - Friends, neighbours and enemies an overview of th.pdf:application/pdf},
}

