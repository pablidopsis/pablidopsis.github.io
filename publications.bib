@article{delk_cml24_2005,
	title = {{CML24}, {Regulated} in {Expression} by {Diverse} {Stimuli}, {Encodes} a {Potential} {Ca2}+ {Sensor} {That} {Functions} in {Responses} to {Abscisic} {Acid}, {Daylength}, and {Ion} {Stress}},
	volume = {139},
	issn = {1532-2548, 0032-0889},
	url = {https://academic.oup.com/plphys/article/139/1/240/6113362},
	doi = {10.1104/pp.105.062612},
	abstract = {Abstract
            Changes in intracellular calcium (Ca2+) levels serve to signal responses to diverse stimuli. Ca2+ signals are likely perceived through proteins that bind Ca2+, undergo conformation changes following Ca2+ binding, and interact with target proteins. The 50-member calmodulin-like (CML) Arabidopsis (Arabidopsis thaliana) family encodes proteins containing the predicted Ca2+-binding EF-hand motif. The functions of virtually all these proteins are unknown. CML24, also known as TCH2, shares over 40\% amino acid sequence identity with calmodulin, has four EF hands, and undergoes Ca2+-dependent changes in hydrophobic interaction chromatography and migration rate through denaturing gel electrophoresis, indicating that CML24 binds Ca2+ and, as a consequence, undergoes conformational changes. CML24 expression occurs in all major organs, and transcript levels are increased from 2- to 15-fold in plants subjected to touch, darkness, heat, cold, hydrogen peroxide, abscisic acid (ABA), and indole-3-acetic acid. However, CML24 protein accumulation changes were not detectable. The putative CML24 regulatory region confers reporter expression at sites of predicted mechanical stress; in regions undergoing growth; in vascular tissues and various floral organs; and in stomata, trichomes, and hydathodes. CML24-underexpressing transgenics are resistant to ABA inhibition of germination and seedling growth, are defective in long-day induction of flowering, and have enhanced tolerance to CoCl2, molybdic acid, ZnSO4, and MgCl2. MgCl2 tolerance is not due to reduced uptake or to elevated Ca2+ accumulation. Together, these data present evidence that CML24, a gene expressed in diverse organs and responsive to diverse stimuli, encodes a potential Ca2+ sensor that may function to enable responses to ABA, daylength, and presence of various salts.},
	language = {en},
	number = {1},
	urldate = {2023-03-03},
	journal = {Plant Physiology},
	author = {Delk, Nikkí A. and Johnson, Keith A. and Chowdhury, Naweed I. and Braam, Janet},
	month = sep,
	year = {2005},
	keywords = {read},
	pages = {240--253},
	annote = {Known from other papers





Novel discoveries


CML24 has differences in sequence to CaM which suggest a different target preference.


CML24 has high Ca2+ affinity and undergoes profound Ca2+-induced conformational changes.


Expression of CML24 increases upon perception of cold, and this requires Ca2+ channel activity = CML24 up-regulation by cold is Ca2+ dependent.


CML24 protein levels do not change upon perception of cold.


It could be that nascent CML24 transcripts are not translated, but it may be more plausible that new protein synthesis is not detected by the methods used here. It is also possible that new synthesis replaces degraded protein…




CML24 is expressed in all major organs, but especially guard cells and vascular tissue.


CML24-underexpressing mutants have delayed flowering at 16- and 24-h photoperiods. Based on its phenotype it is likely that CML24 is required for photoperiod-induced flowering.


Questions





},
	file = {Delk et al. - 2005 - CML24 , Regulated in Expression by Diverse .pdf:C\:\\Users\\pagos\\Zotero\\storage\\FBCQ75CH\\Delk et al. - 2005 - CML24 , Regulated in Expression by Diverse .pdf:application/pdf},
}

@article{ji_arabidopsis_2015,
	title = {The {Arabidopsis} {RCC1} {Family} {Protein} {TCF1} {Regulates} {Freezing} {Tolerance} and {Cold} {Acclimation} through {Modulating} {Lignin} {Biosynthesis}},
	volume = {11},
	issn = {1553-7404},
	url = {https://dx.plos.org/10.1371/journal.pgen.1005471},
	doi = {10.1371/journal.pgen.1005471},
	abstract = {Cell water permeability and cell wall properties are critical to survival of plant cells during freezing, however the underlying molecular mechanisms remain elusive. Here, we report that a specifically cold-induced nuclear protein, Tolerant to Chilling and Freezing 1 (TCF1), interacts with histones H3 and H4 and associates with chromatin containing a target gene, BLUE-COPPER-BINDING PROTEIN (BCB), encoding a glycosylphosphatidylinositolanchored protein that regulates lignin biosynthesis. Loss of TCF1 function leads to reduced BCB transcription through affecting H3K4me2 and H3K27me3 levels within the BCB gene, resulting in reduced lignin content and enhanced freezing tolerance. Furthermore, plants with knocked-down BCB expression (amiRNA-BCB) under cold acclimation had reduced lignin accumulation and increased freezing tolerance. The pal1pal2 double mutant (lignin content reduced by 30\% compared with WT) also showed the freezing tolerant phenotype, and TCF1 and BCB act upstream of PALs to regulate lignin content. In addition, TCF1 acts independently of the CBF (C-repeat binding factor) pathway. Our findings delineate a novel molecular pathway linking the TCF1-mediated cold-specific transcriptional program to lignin biosynthesis, thus achieving cell wall remodeling with increased freezing tolerance.},
	language = {en},
	number = {9},
	urldate = {2023-03-03},
	journal = {PLOS Genetics},
	author = {Ji, Hongtao and Wang, Youning and Cloix, Catherine and Li, Kexue and Jenkins, Gareth I. and Wang, Shuangfeng and Shang, Zhonglin and Shi, Yiting and Yang, Shuhua and Li, Xia},
	editor = {Copenhaver, Gregory P.},
	month = sep,
	year = {2015},
	keywords = {read},
	pages = {e1005471},
	annote = {Known from other papers


RCC1 protein family are guanine nucleotide exchange factors (GEF) for the small GTP-binding protein Ran.


RCC1 is localized in the nucleus, binds to chromatin and generates a Ran-GTP/Ran-GDP gradient across the nuclear envelope which drives nucleo-cytoplasmic transport…


BCB is involved in lignin content and accumulation.


PALs encode isoforms of a key enzyme involved in lignin biosynthesis.


Novel discoveries


TCF1 is named in this paper. It is part of the RCC1 family of chromatin regulators. It has six predicted tandem RCC1 repeats.


TCF1 is specifically induced to cold stress (4C).


TCF1 protein localizes to the nucleus.


TCF1-GFP fusion protein level is also induced by cold. 


TCF1 binds strongly to a histone, it has high affinity towards H3 and H4. Y2H confirms it binds with the specific histone H4 (HFO2, at5g59690).


TCF1 has negligible GEF (guanine exchange factor) activity.


TCF1 does not regulate expression of CBF1-3 or downstream genes from the CBF regulon COR15A and COR47.


CBF1-3 do not regulate expression of TCF1.


GeneChip arrays are used to confirm that TCF1 functions in a novel pathway independent of CBF-COR cascade.


TCF1 does not regulate RD29A expression.


TCF1 associates with the chromatin of BCB.


BCB expression is induced in cold.


TCF1 regulates the transcription of BCB through histone modification, specifically through methylation of H3 at lysine 27?


BCB maintains steady lignin content in Arabidopsis under cold stress, possibly though regulation of PALs.


Questions


Read reference 14 for RD29A, HOS15


},
	file = {Ji et al. - 2015 - The Arabidopsis RCC1 Family Protein TCF1 Regulates.pdf:C\:\\Users\\pagos\\Zotero\\storage\\NQAI64LG\\Ji et al. - 2015 - The Arabidopsis RCC1 Family Protein TCF1 Regulates.pdf:application/pdf},
}

