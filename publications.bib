	keywords = {Phenotype, Arabidopsis, Arabidopsis Proteins, Flowers, Gene Expression Regulation, Developmental, Gene Expression Regulation, Plant, MADS Domain Proteins, Meristem, Models, Biological, Photoperiod, FLC, Flowering, Signal Transduction, Transcription Factors, Plants, Genetically Modified, SOC1, Reproduction, FUL, MADS-box factors., SVP},
	pages = {1193--1203},
	file = {Texto completo:C\:\\Users\\pagos\\Zotero\\storage\\QMFBRRKP\\Balanzà et al. - 2014 - Sequential action of FRUITFULL as a modulator of t.pdf:application/pdf},
