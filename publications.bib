	file = {Full Text PDF:C\:\\Users\\pagos\\Zotero\\storage\\FI9PUS7B\\Zhu et al. - 2020 - TERMINAL FLOWER 1-FD complex target genes and comp.pdf:application/pdf;Snapshot:C\:\\Users\\pagos\\Zotero\\storage\\8VY4EDMA\\s41467-020-18782-1.html:text/html},
}

@article{hanano_arabidopsis_2011,
