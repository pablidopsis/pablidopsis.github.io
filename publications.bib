	url = {http://rgdoi.net/10.13140/RG.2.2.26759.19368},
	doi = {10.13140/RG.2.2.26759.19368},
	abstract = {Lucerne yield is affected by the amount of intercepted radiation and the efficiency with which is used (RUE). The canopy structure of lucerne is a function of the leaf area index (LAI) which can be divided into development (leaf appearance and branching) and growth (leaf area and shoot population) components. Leaf appearance rates (LAR) depend on temperature, but phyllochron is usually stable for similar photoperiods and declines as days get shorter (60 to 34°Cd). Branching is an important yield factor in lucerne and, as opposed to LAR, appears to increase in shorter photoperiods. Plant declines in lucerne are compensated by increases in shoots/plant but there is a threshold of plant density below which yield is compromised (43-132 plants/m2). Leaf area expansion rate (LAER) is the most plastic component of canopy development, and is mostly affected by water stress and partitioning to roots. Compared to other pastures, lucerne is better able to tolerate shade and has the potential to be included in a silvopastoral system. Including trees in dairy farms could decrease heat stress, thus increasing milk production and the farm revenue. However, more information is needed to understand the behavior of lucerne crops under tree canopies in subtropical environments and make more accurate recommendations.},
	language = {en},
	urldate = {2023-05-10},
	author = {Jáuregui, José Martín},
	year = {2016},
	note = {Publisher: Unpublished},
	file = {Jáuregui - 2016 - Factors that affect lucerne canopy development and.pdf:C\:\\Users\\pagos\\Zotero\\storage\\9FE2N2HX\\Jáuregui - 2016 - Factors that affect lucerne canopy development and.pdf:application/pdf},
}

