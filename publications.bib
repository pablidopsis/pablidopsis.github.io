	pages = {D1054--D1059},
	file = {Texto completo:C\:\\Users\\pagos\\Zotero\\storage\\4MBJ9KFK\\Seren et al. - 2017 - AraPheno a public database for Arabidopsis thalia.pdf:application/pdf},
