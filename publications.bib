@article{tiwari_athsc70-1_2020,
	title = {{AtHsc70}-1 negatively regulates the basal heat tolerance in {Arabidopsis} thaliana through affecting the activity of {HsfAs} and {Hsp101}},
	volume = {103},
	issn = {1365-313X},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/tpj.14883},
	doi = {10.1111/tpj.14883},
	abstract = {Heat shock protein 70 (Hsp70) chaperones are highly conserved and essential proteins with diverse cellular functions, including plant abiotic stress tolerance. Hsp70 proteins have been linked with basal heat tolerance in plants. Hsp101 likewise is an important chaperone protein that plays a critical role in heat tolerance in plants. We observed that Arabidopsis hsc70-1 mutant seedlings show elevated basal heat tolerance compared with wild-type. Over-expression of Hsc70-1 resulted in increased heat sensitivity. Hsp101 transcript and protein levels were increased during non-heat stress (HS) and post-HS conditions in hsc70-1 mutant seedlings. In contrast, Hsp101 was repressed in Hsc70-1 over-expressing plants after post-HS conditions. Hsc70-1 showed physical interaction with HsfA1d and HsfA1e protein in the cytosol under non-HS conditions. In transient reporter gene analysis, HsfA1d, HsfA1e and HsfA2 showed transcriptional response on the Hsp101 promoter. HsfA1d and HsfA2 transcripts were at higher levels in hsc70-1 mutant compared with wild-type. We provide genetic evidence that Hsc70-1 is a negative regulator affecting HsfA1d/A1e/A2 activators, which in turn regulate Hsp101 expression and basal thermotolerance.},
	language = {en},
	number = {6},
	urldate = {2023-03-03},
	journal = {The Plant Journal},
	author = {Tiwari, Lalit D. and Khungar, Lisha and Grover, Anil},
	year = {2020},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/tpj.14883},
	keywords = {Arabidopsis, thermotolerance, heat stress, HsfA1d, HsfA2, Hsp101, Hsp70, negative regulator},
	pages = {2069--2083},
	file = {Full Text PDF:C\:\\Users\\pagos\\Zotero\\storage\\KGT2DVGS\\Tiwari et al. - 2020 - AtHsc70-1 negatively regulates the basal heat tole.pdf:application/pdf;Snapshot:C\:\\Users\\pagos\\Zotero\\storage\\FD6ZPXN8\\tpj.html:text/html},
