	year = {2005},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.3732/ajb.92.3.432},
	keywords = {germination, Asteraceae, population, seed dimorphism, seed morphology, seed size, seedling growth, The Netherlands},
	pages = {432--437},
	file = {Full Text PDF:C\:\\Users\\pagos\\Zotero\\storage\\DDVW25FE\\van Mölken et al. - 2005 - Only seed size matters for germination in differen.pdf:application/pdf;Snapshot:C\:\\Users\\pagos\\Zotero\\storage\\AUB26N9K\\ajb.92.3.html:text/html},
}

@article{murali_patterns_1997,
