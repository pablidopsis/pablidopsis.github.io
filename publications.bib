	pages = {1141--1151},
	file = {Thines et al. - 2014 - The time of day effects of warm temperature on flo.pdf:C\:\\Users\\pagos\\Zotero\\storage\\R7GHQPXB\\Thines et al. - 2014 - The time of day effects of warm temperature on flo.pdf:application/pdf},
}

