@article{bond_low_2011,
	title = {The low temperature response pathways for cold acclimation and vernalization are independent},
	volume = {34},
	issn = {1365-3040},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/j.1365-3040.2011.02370.x},
