	pages = {863--876},
	file = {Full Text PDF:C\:\\Users\\pagos\\Zotero\\storage\\5BK3U9SV\\Hensel et al. - 1994 - The Fate of Inflorescence Meristems Is Controlled .pdf:application/pdf;Snapshot:C\:\\Users\\pagos\\Zotero\\storage\\GU5RXYAW\\6068706.html:text/html},
}

@article{liu_post-fertilization_2014,
	title = {Post-fertilization expression of {FLOWERING} {LOCUS} {T} suppresses reproductive reversion},
	volume = {5},
	issn = {1664-462X},
	url = {https://www.frontiersin.org/article/10.3389/fpls.2014.00164},
	abstract = {FLOWERING LOCUS T (FT) encodes a systemic signal communicating the perception of long day photoperiod from leaves to the shoot apex to induce the floral transition. Transient expression of FT in the phloem companion cells of rosette leaves for one to several days was previously shown to be sufficient to commit plants to flowering. Here we show that partial commitment results in pleiotropic inflorescence meristem reversion phenotypes. FT expression is much stronger in organs formed after the floral transition such as cauline leaves, sepals, and developing siliques. We show that expression of FT and its paralog TWIN SISTER OF FT (TSF) after the floral transition plays a role in inflorescence meristem stabilization even if plants flower very late in development. CONSTANS (CO), the major activator of FT, is not required to prevent late reproductive reversion. The requirement for FT is temporal since reproductive reversion to a vegetative state occurs only in recently formed inflorescence meristems. Unlike for the expression of FT in leaves, neither the distal putative FT enhancer nor long-day photoperiod is required for FT expression in developing siliques. Expression of FT in developing siliques and their supporting stems is sufficient to stabilize flowering during the sensitive developmental window indicating that fruit generated FT participates in inflorescence stabilization.},
	urldate = {2022-01-27},
	journal = {Frontiers in Plant Science},
	author = {Liu, Liangyu and Farrona, Sara and Klemme, Sonja and Turck, Franziska K.},
	year = {2014},
	annote = {Some insights into FT roles after the onset of flowering.},
	file = {Full Text PDF:C\:\\Users\\pagos\\Zotero\\storage\\XSMXELLU\\Liu et al. - 2014 - Post-fertilization expression of FLOWERING LOCUS T.pdf:application/pdf},
}

