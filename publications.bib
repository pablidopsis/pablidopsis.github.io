	keywords = {read, key},
	pages = {1219--1232},
	file = {Benedict et al. - 2006 - Consensus by Democracy. Using Meta-Analyses of Mic.pdf:C\:\\Users\\pagos\\Zotero\\storage\\AV8JZKLS\\Benedict et al. - 2006 - Consensus by Democracy. Using Meta-Analyses of Mic.pdf:application/pdf},
