	month = dec,
	year = {2021},
	pages = {864},
	file = {Masuda et al. - 2021 - The singularity response reveals entrainment prope.pdf:C\:\\Users\\pagos\\Zotero\\storage\\6IQ89JUV\\Masuda et al. - 2021 - The singularity response reveals entrainment prope.pdf:application/pdf},
}

