@article{chen_bypass1-like_2019,
	title = {{BYPASS1}-{LIKE}, {A} {DUF793} {Family} {Protein}, {Participates} in {Freezing} {Tolerance} via the {CBF} {Pathway} in {Arabidopsis}},
	volume = {10},
	issn = {1664-462X},
	url = {https://www.frontiersin.org/articles/10.3389/fpls.2019.00807},
	abstract = {The C-REPEAT BINDING FACTOR signaling pathway is strictly modulated by numerous factors and is essential in the cold response of plants. Here, we show that the DUF793 family gene BYPASS1-LIKE modulates freezing tolerance through the CBFs in Arabidopsis. The expression of B1L was rapidly induced under cold treatment. Comparing to wild type, B1L knockout mutants were more sensitive to freezing treatment, whereas B1L-overexpressing lines were more tolerant. The expression of CBFs and CBF target genes was significantly decreased in b1l mutant. Using yeast two-hybrid screening system, 14-3-3λ was identified as one of proteins interacting with B1L. The interaction was confirmed with bimolecular fluorescence complementation assay and co-immunoprecipitation assay. Biochemical assays revealed that b1l mutation promoted the degradation of CBF3 compared to wild type, whereas 14-3-3κλ mutant and b1l 14-3-3κλ mutant suppressed the degradation of CBF3. Consistently, 14-3-3κλ and b1l 14-3-3κλ mutants showed enhanced freezing tolerance compared to wild type. These results indicate that B1L enhances the freezing tolerance of plants, at least partly through stabilizing CBF. Our findings improve our understanding of the regulation of CBF in response to cold stress.},
	urldate = {2023-02-27},
	journal = {Frontiers in Plant Science},
	author = {Chen, Tao and Chen, Jia-Hui and Zhang, Wei and Yang, Gang and Yu, Li-Juan and Li, Dong-Ming and Li, Bo and Sheng, Hong-Mei and Zhang, Hua and An, Li-Zhe},
	year = {2019},
	annote = {

B1L transcription is upregulated by cold within hours.


B1L regulates CBF transcription - in the b1l mutant there is less CBF1-3 transcript level after 3 hours of cold (compared to wt).


I am not convinced that it is explained how this happens.




Transcriptional regulation of CBF by B1L is dependent on 14-3-3, in the double mutant the CBF transcript level is similar to wt.


B1L regulates CBF post-transcriptionally, it binds to 14-3-3 decreasing the degradation of CBF1-3 protein by 14-3-3s.


This is all proved.




The final model does not really include the transcriptional regulation, why?


},
	file = {Full Text PDF:C\:\\Users\\pagos\\Zotero\\storage\\VJSPK9XU\\Chen et al. - 2019 - BYPASS1-LIKE, A DUF793 Family Protein, Participate.pdf:application/pdf},
}

@article{lee_inducer_2015,
	title = {{INDUCER} {OF} {CBF} {EXPRESSION} 1 integrates cold signals into {FLOWERING} {LOCUS} {C}-mediated flowering pathways in {Arabidopsis}},
	volume = {84},
	issn = {1365-313X},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/tpj.12956},
	doi = {10.1111/tpj.12956},
	abstract = {Plants constantly monitor changes in photoperiod and temperature throughout the year to synchronize flowering with optimal environmental conditions. In the temperate zones, both photoperiod and temperature fluctuate in a somewhat predictable manner through the seasons, although a transient shift to low temperature is also encountered during changing seasons, such as early spring. Although low temperatures are known to delay flowering by inducing the floral repressor FLOWERING LOCUS C (FLC), it is not fully understood how temperature signals are coordinated with photoperiodic signals in the timing of seasonal flowering. Here, we show that the cold signaling activator INDUCER OF CBF EXPRESSION 1 (ICE1), FLC and the floral promoter SUPPRESSOR OF OVEREXPRESSION OF CONSTANS 1 (SOC1) constitute an elaborate signaling network that integrates cold signals into flowering pathways. The cold-activated ICE1 directly induces the gene encoding FLC, which represses SOC1 expression, resulting in delayed flowering. In contrast, under floral promotive conditions, SOC1 inhibits the binding of ICE1 to the promoters of the FLC gene, inducing flowering with a reduction of freezing tolerance. These observations indicate that the ICE1-FLC-SOC1 signaling network contributes to the fine-tuning of flowering during changing seasons.},
	language = {en},
	number = {1},
	urldate = {2023-02-27},
	journal = {The Plant Journal},
	author = {Lee, Jae-Hyung and Jung, Jae-Hoon and Park, Chung-Mo},
	year = {2015},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/tpj.12956},
	keywords = {FLC, Arabidopsis thaliana, cold acclimation, flowering time, ICE1, SOC1},
	pages = {29--40},
	file = {Full Text PDF:C\:\\Users\\pagos\\Zotero\\storage\\4RC4W3B4\\Lee et al. - 2015 - INDUCER OF CBF EXPRESSION 1 integrates cold signal.pdf:application/pdf;Snapshot:C\:\\Users\\pagos\\Zotero\\storage\\L7EJPVY9\\tpj.html:text/html},
