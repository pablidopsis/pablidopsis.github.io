@article{steinfort_vernalisation_2017,
	title = {Vernalisation and photoperiod sensitivity in wheat: {The} response of floret fertility and grain number is affected by vernalisation status},
	volume = {203},
	issn = {0378-4290},
	shorttitle = {Vernalisation and photoperiod sensitivity in wheat},
	url = {https://www.sciencedirect.com/science/article/pii/S0378429016305172},
	doi = {10.1016/j.fcr.2016.10.013},
	abstract = {The impact of different allelic combinations of VERNALIZATION1 (VRN1) and PHOTOPERIOD1 (PPD1) on the development of reproductive structures, spike dry weight (SDW) and its links to the duration of stem elongation (SE) and yield components was assessed using Near Isogenic Lines (NILs) of the cultivar Sunstate for both genes. Emphasis was on observations on the main shoot and tillers, linking to plant and canopy level in Steinfort at al. (2016). Experiments under controlled and irrigated field conditions relevant to the northern wheat growing region of Australia were designed. In vernalised plants, the number of fertile florets in the main shoot spike was positively related to the number of fertile spikelets, the duration of SE, the SDW at anthesis and the sum of the radiation intercepted during SE, while no associations were found under non vernalised and slow vernalising field conditions. Vernalised plants also produced more grains in the average tiller spike, compared to non vernalised plants, particularly under short days. Both main shoot and tiller grain number per spike contributed to the higher grain number per plant observed in vernalised plants in Steinfort et al. (2016), as spike number was similar. In the field, higher yield of lines with spring alleles of VRN1 was underpinned by longer spikes, with fewer spikelets but more fertile florets per spikelet in the main shoot spike, and more grains per tiller. When non vernalised, winter lines had an apex that grew slower, shorter spikes and higher spikelet density compared to the other genotypes. The utilisation of sensitivity to VRN1 or PPD1 to increase the duration of the SE and the number of fertile florets and grains is a more complex process than originally proposed.},
	urldate = {2023-09-12},
	journal = {Field Crops Research},
	author = {Steinfort, Ursula and Fukai, Shu and Trevaskis, Ben and Glassop, Donna and Chan, Amy and Dreccer, M. Fernanda},
	month = mar,
	year = {2017},
	keywords = {read, vernalization, Phenology, Wheat, Fertile florets, Grain number, PPD1, Stem elongation, VRN1, Yield},
	pages = {243--255},
	annote = {Findings


Vernalization decreases the number of spikelets.


Vernalization increases the maximum number of floret primordia IN BASAL SPIKELETS.


Vernalization increases the number of fertile florets in basal but not in central or apical spikelets.


In vernalized plants, basal spikelets had floret primordia that were more advanced in development compared to non-vernalized plants.


Higher number of florets per spike when plants are vernalized under short days.


"Regarding floret development, there was only an effect of vernalisation on the number of initiated florets at DC49 and the number of fertile florets at DC65 on the basal spikelet." (Steinfort et al., 2017, p. 251)


},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\pagos\\Zotero\\storage\\F5AD2M7X\\Steinfort et al. - 2017 - Vernalisation and photoperiod sensitivity in wheat.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\pagos\\Zotero\\storage\\HCFBDG7M\\S0378429016305172.html:text/html},
}

@article{leithold_investigations_1997,
	title = {Investigations on {Heat} {Tolerance} of {Spring} {Wheat} {Varieties} of {Different} {Origin} {Under} {Growth} {Chamber} {Conditions}},
	volume = {179},
	issn = {1439-037X},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/j.1439-037X.1997.tb00506.x},
	doi = {10.1111/j.1439-037X.1997.tb00506.x},
	abstract = {Winter wheat has replaced spring wheat to a large extent under the climatic conditions of Western and Central Europe. But spring wheat genotypes are still important under the warm climate in large parts of Africa and Central America, since spring wheat needs no vernalization. To reach optimal yield, genotypes must be well adapted to heat stress. In this study 11 spring wheat varieties of Egypt, Sudan, CIMMYT and Germany were exposed to high temperatures at ear emergence and beginning of anthesis. There were reductions in kernel yield and kernel number of the main ear for all genotypes, but the differences in reduction were remarkable. The German variety ‘Naxos’ was best adapted. Some varieties showed an increased number of florets per spikelet under heat treatment, but this does not prohibit a reduction in kernel number per spikelet.},
	language = {en},
	number = {2},
	urldate = {2023-09-12},
	journal = {Journal of Agronomy and Crop Science},
	author = {Leithold, B. and Müller, G. and Weber, W. E. and Westermann, T.},
	year = {1997},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/j.1439-037X.1997.tb00506.x},
	keywords = {read, temperature, floret number, grain yield, high temperature tolerance, kernel number, Triticum aestivum},
	pages = {115--122},
	annote = {Findings


Heat stress (40ºC for 7 days) applied at DC47 or DC61 affected the number of florets per spikelet differently for different varieties.


In ‘Gemiza 1’, heat stress increased the number of florets. 


In ‘Naxos’ too, but only when it was applied at DC47.


In ‘Wadi El Nile’, there was no effect.




},
	file = {Full Text PDF:C\:\\Users\\pagos\\Zotero\\storage\\7BMEWSVN\\Leithold et al. - 1997 - Investigations on Heat Tolerance of Spring Wheat V.pdf:application/pdf;J Agronomy Crop Science - 2008 - Leithold.pdf:C\:\\Users\\pagos\\Zotero\\storage\\LGUG9CM6\\J Agronomy Crop Science - 2008 - Leithold.pdf:application/pdf;Snapshot:C\:\\Users\\pagos\\Zotero\\storage\\46UZRWWR\\j.1439-037X.1997.tb00506.html:text/html},
}

@article{prieto_dynamics_2018,
	title = {Dynamics of floret initiation/death determining spike fertility in wheat as affected by {Ppd} genes under field conditions},
	volume = {69},
	issn = {0022-0957},
	url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC5920323/},
	doi = {10.1093/jxb/ery105},
	abstract = {Ppd-1a reduced the number of fertile florets due largely to lowering floret survival, and the effect was not proportional to the duration of the late reproductive phase., As wheat yield is linearly related to grain number, understanding the physiological determinants of the number of fertile florets based on floret development dynamics due to the role of the particular genes is relevant. The effects of photoperiod genes on dynamics of floret development are largely ignored. Field experiments were carried out to (i) characterize the dynamics of floret primordia initiation and degeneration and (ii) to determine which are the most critical traits of such dynamics in establishing genotypic differences in the number of fertile florets at anthesis in near isogenic lines (NILs) carrying photoperiod-insensitive alleles. Results varied in magnitude between the two growing seasons, but in general introgression of Ppd-1a alleles reduced the number of fertile florets. The actual effect was affected not only by the genome and the doses but also by the source of the alleles. Differences in the number of fertile florets were mainly explained by differences in the floret generation/degeneration dynamics, and in most cases associated with floret survival. Manipulating photoperiod insensitivity, unquestionably useful for changing flowering time, may reduce spike fertility but much less than proportionally to the change in duration of development, as the insensitivity alleles did increase the rate of floret development.},
	number = {10},
	urldate = {2023-09-12},
	journal = {Journal of Experimental Botany},
	author = {Prieto, Paula and Ochagavía, Helga and Savin, Roxana and Griffiths, Simon and Slafer, Gustavo A},
	month = apr,
	year = {2018},
	pmid = {29562264},
	pmcid = {PMC5920323},
	pages = {2633--2645},
	file = {PubMed Central Full Text PDF:C\:\\Users\\pagos\\Zotero\\storage\\NGI2N7E3\\Prieto et al. - 2018 - Dynamics of floret initiationdeath determining sp.pdf:application/pdf},
}

@article{li_dynamic_2001,
	title = {Dynamic characteristics of floret primordium development in wheat},
	abstract = {This study was undertaken to determine the time-course of ¯oret primordium initiation and degeneration in relation to thermal time after sowing in various cultivars and tiller positions of wheat (Triticum aestivum L.). The ®eld experiment was carried out on the experiment station of Nanjing Agricultural University of China, and included three sowing dates: early planting of 30 September, middle planting of 30 October and late planting of 2 March and two wheat cultivars: winter type `Jing 411' and spring type `Yangmai 158'. Quadratic and linear relationships could describe ¯oret initiation, degeneration and abortion in response to growing-degree-days after sowing for main stem and ®rst and second tillers of middle planting plants of both cultivars. The ¯oret development patterns on the third tillers for all treatments deviated from the basic models and were associated with poor ¯oret fertility. The growing-degree-days from sowing to ¯oret initiation were signi®cantly different between the three sowing dates and between the two cultivars. These results help quantify the dynamic processes of apical development and explain the differences in ¯oret development of wheat with different environments, cultivars and tiller positions. \# 2001 Elsevier Science B.V. All rights reserved.},
	language = {en},
	journal = {Field Crops Research},
	author = {Li, Cundong and Cao, Weixing and Dai, Tingbo},
	year = {2001},
	keywords = {read, vernalization},
	annote = {Findings


Three characteristic phases of floret development:


Initiation (I), with gradually decreasing rate


Degeneration (II), with high and stable rate


Abortion (III), negligible




Floret development is similar between the main stem and tillers.





The effect of vernalization on floret development depends on the genotype (winter or spring accession).


Late planting (no vernalization) decreases the total number of florets per spike in both winter and spring accessions.


When planted early (September, with vernalization), floret numbers are higher in the winter accession (presumably because it vernalized). When planted late (March, no vernalization), they are higher in the spring accession.


Duration of floret initiation (in GDD) is longer in the spring accession for the middle planting. For the late planting, it is similar for both accessions.


Vernalization decreases the duration of floret initiation, only in the winter accession.




},
	file = {Li et al. - 2001 - Dynamic characteristics of ¯oret primordium develo.pdf:C\:\\Users\\pagos\\Zotero\\storage\\FI3MSZ8K\\Li et al. - 2001 - Dynamic characteristics of ¯oret primordium develo.pdf:application/pdf},
}

@article{miralles_developmental_2001,
	title = {Developmental responses to sowing date in wheat, barley and rapeseed},
	volume = {71},
	issn = {03784290},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S0378429001001617},
	doi = {10.1016/S0378-4290(01)00161-7},
	abstract = {Two cultivars of each bread wheat (Triticum aestivum L.), malting barley (Hordeum vulgare L.) and spring rapeseed (Brassica napus L.), were grown in a wide range of different sowing dates in Buenos Aires, Argentina. The aims of this study were (i) to analyse how the number of leaf primordia and the rate of leaf emergence interact to modify the duration of different pre-¯owering phases and (ii) to establish how different yield components initiated during those phases are affected when the duration of these phases are modi®ed by wide range of sowing dates. The results showed interactions between photoperiod and temperature for wheat and rapeseed producing different combination for the length of the pre-¯owering phases. Barley reached ¯oral initiation earlier than wheat and rapeseed and had a shorter `intrinsic earliness'.},
