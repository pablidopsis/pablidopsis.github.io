	pages = {155--158},
	file = {Miralles et al. - 2003 - Influence of ‘historic’ photoperiod during stem el.pdf:C\:\\Users\\pagos\\Zotero\\storage\\Q5CPQIL9\\Miralles et al. - 2003 - Influence of ‘historic’ photoperiod during stem el.pdf:application/pdf},
}

