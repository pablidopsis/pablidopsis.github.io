	author = {Singh, Vijay and Louis, Joe and Ayre, Brian G. and Reese, John C. and Shah, Jyoti},
	year = {2011},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/j.1365-313X.2011.04583.x},
	keywords = {anti-xenosis, antibiosis, green peach aphid, PAD4, plant defense, trehalose metabolism},
	pages = {94--104},
