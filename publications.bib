	keywords = {Mutation, Arabidopsis, Arabidopsis Proteins, Flowers, Gene Expression Regulation, Plant, MADS Domain Proteins, Transcription Factors, Cell Nucleus, Methylation, Polycomb-Group Proteins, Protein Transport, Repressor Proteins, Up-Regulation},
	pages = {1667--1678},
