	file = {Full Text PDF:C\:\\Users\\pagos\\Zotero\\storage\\TZ5PCHVW\\Meng et al. - 2017 - Type-B ARABIDOPSIS RESPONSE REGULATORs Specify the.pdf:application/pdf;Snapshot:C\:\\Users\\pagos\\Zotero\\storage\\5HQP59UH\\6099327.html:text/html},
